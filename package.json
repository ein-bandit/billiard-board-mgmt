{
  "name": "bm3001",
  "version": "1.0.0",
  "description": "",
  "productName": "Billiard Manager 3001",
  "keywords": [],
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "ia32"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "react": "16.3.2",
    "react-dom": "16.3.3",
    "react-modal": "^3.11.1",
    "react-scripts": "1.1.4"
  },
  "devDependencies": {
    "electron": "^2.0.6",
    "electron-builder": "^20.26.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron": "set ELECTRON_INDEX=/../build/index.html && electron .",
    "electron-dev": "set ELECTRON_START_URL=http://localhost:3000 && electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  }
}
